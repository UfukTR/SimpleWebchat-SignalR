@model List<SimpleWebchat.BLL.ViewModels.MessageViewmodel>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Message</title>
</head>
<body>
    <div> 
        <table id="datatable-buttons" class="table table-striped table-bordered">
            <thead>
                <tr>
                    <th>UserId</th>
                    <th>Message</th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.UserID</td>
                        <td>@item.MessageText</td>
                    </tr>
                }


            </tbody>

        </table>





    </div>
</body>
</html>
